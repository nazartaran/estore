<div class='container'>
  <h1>Your busket</h1>
    <table class='table'>
      <tr>
        <th>Product</th>
        <th>Quantity</th>
      </tr> 
      <% @busket.each do |b| %>
      <tr class='item'>
        <td><%= b.product_name %></td>
        <td><%= b.quantity %></td>
        <td><div class='delete_icon' id='<%=b.id%>'></div></td> 
      </tr>
      <%end%>
    </table>
  <%= link_to 'Order', new_order_path %>
</div>
